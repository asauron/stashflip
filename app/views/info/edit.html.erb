<% form_for :info do |form| %>
<fieldset>
	Edit Information<br/>
	<% error_messages_for 'info' %>
	<%= form_field_maker(form, "name", "Name", 20, 20)%>
	<%= form_field_maker(form, "location", "Location", 40, 40)%>
	<%= form_field_maker(form, "bio", "Tagline", 140, 140)%>
	
	<%= submit_tag "Save", :class => "submit"%>
</fieldset>
<% end %>
	
	
	
	