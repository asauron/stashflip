<html>

<head>
	<title>Stashflip - BUY AND RESELL</title>
	<link rel="stylesheet" type="text/css" href="style.css" />
</head>

<body>
	<div id="search_bar">
		<%= render :partial => 'searchbar' %>		 
	</div>
	
	<div id="maincontent">
		
	    	  	<form name="post_deal" id="post_deal" class="post_deal" method="post" action="/deals/post_deal">  
			<!--need this for auth -->
            <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token%>" /> 
		 <%= submit_tag("Refresh Deals (RSS)", :id => "post_deal", :title => "post_deal", :name => "post_deal", :class => "red awesome small button")%> 
		 
			<div id="main_top">
				<%= render :partial => '/deals/latest_sponsored' %>				
				<%= render :partial => '/deals/latest_passwird' %>
			</div> 
			
			<div id="main_right">
				<%= render :partial => '/deals/adroll' %>
			</div>     	        	  	

		<%= render :partial => 'website/statcounter' %>
		<%= render :partial => 'website/footer' %>

	</div><!--end maincontent-->
	
</body>

